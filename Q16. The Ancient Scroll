// Q16.java
// Linear search for key. Input: n then n integers then key. Output: index (0-based) or -1
import java.util.*;
public class Q16 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt(); int[] a=new int[n];
        for(int i=0;i<n;i++) a[i]=s.nextInt();
        int key=s.nextInt();
        int idx=-1;
        for(int i=0;i<n;i++) if(a[i]==key){ idx=i; break; }
        System.out.println(idx);
    }
}
