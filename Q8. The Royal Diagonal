// Q8.java
// Sum of both diagonals for square matrix. Input: n n then n*n values. Output: "d1sum d2sum" or formatted.
import java.util.*;
public class Q8 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int r=s.nextInt(), c=s.nextInt();
        int n=r;
        long d1=0, d2=0;
        int[][] a=new int[n][n];
        for(int i=0;i<n;i++) for(int j=0;j<n;j++) a[i][j]=s.nextInt();
        for(int i=0;i<n;i++){
            d1 += a[i][i];
            d2 += a[i][n-1-i];
        }
        System.out.println(d1 + " " + d2);
    }
}
