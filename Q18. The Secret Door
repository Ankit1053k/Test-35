// Q18.java
// Binary search. Input: n then sorted n ints then key. Output: index (0-based) or -1
import java.util.*;
public class Q18 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt(); int[] a=new int[n];
        for(int i=0;i<n;i++) a[i]=s.nextInt();
        int key=s.nextInt();
        int l=0,r=n-1;
        while(l<=r){
            int m = l + (r-l)/2;
            if(a[m]==key){ System.out.println(m); return; }
            else if(a[m]<key) l=m+1;
            else r=m-1;
        }
        System.out.println(-1);
    }
}
