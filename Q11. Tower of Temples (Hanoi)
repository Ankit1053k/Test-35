// Q11.java
// Tower of Hanoi moves count. Input: n. Output: number of moves (2^n - 1)
import java.util.*;
public class Q11 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        long moves = (1L<<n) - 1L;
        System.out.println(moves);
    }
}
