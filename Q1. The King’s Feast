import java.util.*;
public class Q1 {
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        long max = Long.MIN_VALUE;
        for(int i=0;i<n;i++){
            long v = s.nextLong();
            if(v>max) max=v;
        }
        System.out.println(max);
    }
}
