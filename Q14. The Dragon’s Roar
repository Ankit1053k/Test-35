// Q14.java
// Print 1..n using recursion. Input: n. Output: numbers space separated.
import java.util.*;
public class Q14 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        print(1,n);
        System.out.println();
    }
    static void print(int cur,int n){
        if(cur>n) return;
        System.out.print(cur + (cur==n? "": " "));
        print(cur+1,n);
    }
}
