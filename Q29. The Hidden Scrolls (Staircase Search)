// Q29.java
// Staircase search (start from top-right). Input: r c then matrix then target. Output: True/False
import java.util.*;
public class Q29 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int r=s.nextInt(), c=s.nextInt();
        int[][] m=new int[r][c];
        for(int i=0;i<r;i++) for(int j=0;j<c;j++) m[i][j]=s.nextInt();
        int target=s.nextInt();
        int i=0,j=c-1;
        boolean ok=false;
        while(i<r && j>=0){
            if(m[i][j]==target){ ok=true; break; }
            else if(m[i][j]>target) j--;
            else i++;
        }
        System.out.println(ok);
    }
}
