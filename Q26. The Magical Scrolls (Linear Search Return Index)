// Q26.java
// Return row and col of target in matrix or (-1,-1). Input: r c then matrix then target.
import java.util.*;
public class Q26 {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int r=s.nextInt(), c=s.nextInt();
        int[][] m=new int[r][c];
        for(int i=0;i<r;i++) for(int j=0;j<c;j++) m[i][j]=s.nextInt();
        int target=s.nextInt();
        for(int i=0;i<r;i++) for(int j=0;j<c;j++) if(m[i][j]==target){ System.out.println("("+i+","+j+")"); return;}
        System.out.println("(-1,-1)");
    }
}
